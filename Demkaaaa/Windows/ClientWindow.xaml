<Window x:Class="Demkaaaa.Windows.ClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demkaaaa.Windows"
        mc:Ignorable="d"
        Title="ClientWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="26*"/>
        </Grid.RowDefinitions>
        <StackPanel>
            
        </StackPanel>
        <ListView Grid.Row="1" x:Name="listCl">
            <ListView.View>
                <GridView >
                    <GridViewColumn Header="dfscv " DisplayMemberBinding="{Binding Path= Client}" />
                    <GridViewColumn Header=" dfg" DisplayMemberBinding="{Binding Path= NameService}" />
                    <GridViewColumn Header="fdgdf " DisplayMemberBinding="{Binding Path= NameProduct}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
